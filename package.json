{
  "name": "mongo-admin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "node bin/move-assets.js",
    "build": "npm run build:js && npm run build:sass && npm run build:pug",
    "postbuild": "node bin/move-assets.js --recipe=post",
    "build:js": "browserify web/src/js/index.js -o web/dist/js/app.js -t [ babelify --presets [ es2015 stage-1 ] ]",
    "build:sass": "node-sass --include-path=$(node bin/sass-paths.js) web/src/sass/style.sass web/dist/css/style.css",
    "build:pug": "pug --out web/dist --pretty web/src/pug/index.pug",
    "watch": "npm run watch:js & npm run watch:sass & npm run watch:pug & npm run livereload",
    "watch:js": "nodemon --watch 'web/src/js' --exec 'npm run build:js'",
    "watch:sass": "npm run build:sass -- --watch web/src/sass/**/*",
    "watch:pug": "npm run build:pug -- --watch",
    "livereload": "livereload web/dist",
    "lint": "npm run lint:server && npm run lint:web",
    "lint:server": "eslint server",
    "lint:web": "eslint web/src/js",
    "start": "npm run build && nodemon server & npm run watch",
    "start:electron": "electron ./electron",
    "pack-linux": "electron-packager ./electron --asar --asar-unpack=protocol-link.html --overwrite --platform=linux --arch=x64 --prune=true --out=electron/dist"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "bourbon": "^4.2.7",
    "bourbon-neat": "^1.8.0",
    "express": "^4.14.0",
    "font-awesome": "^4.6.3",
    "iblokz": "./iblokz",
    "method-override": "^2.3.6",
    "mongoose": "^4.5.7",
    "rx": "^4.1.0",
    "snabbdom": "^0.5.3",
    "superagent": "^2.1.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "electron-packager": "^7.4.0",
    "electron-prebuilt": "^1.3.1",
    "eslint": "^3.1.1",
    "eslint-config-google": "^0.6.0",
    "fs-extra": "^0.30.0",
    "livereload": "^0.5.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.0",
    "pug-cli": "^1.0.0-alpha6"
  },
  "author": "",
  "license": "ISC"
}
