{
  "name": "mongo-admin",
  "version": "1.0.0",
  "description": "",
  "main": "electron.js",
  "scripts": {
    "prebuild": "node bin/move-assets.js",
    "build": "npm run build:js && npm run build:sass",
    "build:js": "browserify src/js/index.js -o dist/web/js/app.js -t [ babelify --presets [ es2015 stage-1 ] ]",
    "build:sass": "node-sass --source-map dist/web/css/style.css.map --include-path=$(node bin/sass-paths.js) src/sass/style.sass dist/web/css/style.css",
    "watch": "npm run watch:js & npm run watch:sass & npm run livereload",
    "watch:js": "watchify -p browserify-hmr src/js/index.js -o dist/web/js/app.js -t [ babelify --presets [ es2015 stage-1 ] ] --debug",
    "watch:sass": "npm run build:sass -- --watch src/sass/**/*",
    "livereload": "livereload dist/web/**/*.css -d",
    "lint": "npm run lint:server && npm run lint:web",
    "lint:server": "eslint server",
    "lint:web": "eslint src/js",
    "start": "npm run watch & nodemon server & npm run start:electron",
    "start:electron": "electron ./",
    "pack": "npm run pack:linux && npm run pack:osx && npm run pack:win32 && npm run pack:win64",
    "pack:linux": "electron-packager ./ mongoAdmin --asar --ignore='dist/(win|linux|osx)' --out=dist/linux --overwrite --platform=linux --arch=x64 --icon=assets/icon-48.png",
    "pack:osx": "electron-packager ./ mongoAdmin --asar  --ignore='dist/(win|linux|osx)' --out=dist/osx --overwrite --platform=darwin --arch=x64 --icon=assets/icon-48.icns",
    "pack:win32": "electron-packager ./ mongoAdmin --asar --ignore='dist/(win|linux|osx)' --out=dist/win --overwrite --platform=win32 --arch=ia32 --icon=assets/icon-48.ico",
    "pack:win64": "electron-packager ./ mongoAdmin --asar  --ignore='dist/(win|linux|osx)' --out=dist/win --overwrite --platform=win32 --arch=x64 --icon=assets/icon-48.ico"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "bourbon": "^4.2.7",
    "bourbon-neat": "^1.8.0",
    "escape-string-regexp": "^1.0.5",
    "express": "^4.14.0",
    "font-awesome": "^4.6.3",
    "iblokz-data": "^1.0.1",
    "iblokz-snabbdom-helpers": "^1.0.0",
    "method-override": "^2.3.6",
    "mongoose": "^4.6.2",
    "rx": "^4.1.0",
    "superagent": "^3.3.1",
    "vex-js": "^3.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.4.1",
    "electron": "^1.6.1",
    "electron-packager": "^8.5.2",
    "eslint": "^3.1.1",
    "eslint-config-google": "^0.6.0",
    "fs-extra": "^0.30.0",
    "livereload": "^0.5.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.0",
    "watchify": "^3.8.0"
  },
  "author": "",
  "license": "ISC"
}
